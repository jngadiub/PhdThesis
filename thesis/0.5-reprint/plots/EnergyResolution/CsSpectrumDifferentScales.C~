   TLegend *leg = new TLegend(0.1537356,0.6927966,0.6034483,0.9491525,NULL,"brNDC");
   leg->SetBorderSize(1);
   //leg->SetTextFont(132);
   //leg->SetTextSize(0.05084746);
   leg->SetLineColor(0);
   leg->SetLineStyle(1);
   leg->SetLineWidth(1);
   leg->SetFillStyle(0);
   leg->SetFillColor(0);
   entry=leg->AddEntry("NULL","S1","");
   //entry->SetLineColor(4);
   //entry->SetLineStyle(1);
   //entry->SetLineWidth(2);
   entry->SetTextColor(4);
   //entry->SetMarkerStyle(1);
   //entry->SetMarkerSize(1);
   entry=leg->AddEntry("NULL","S2","");
   //entry->SetFillStyle(3003);
   //entry->SetLineColor(2);
   //entry->SetLineStyle(2);
   //entry->SetLineWidth(3);
   entry->SetTextColor(2);
   //entry->SetMarkerStyle(1);
   //entry->SetMarkerSize(1);
   entry=leg->AddEntry("Cs","combined scale","");
   entry->SetFillStyle(3003);
   entry->SetLineColor(1);
   entry->SetLineStyle(9);
   entry->SetLineWidth(2);
   entry->SetMarkerColor(1);
   entry->SetMarkerStyle(1);
   entry->SetMarkerSize(1);
   leg->Draw();
   Can_cs->Modified();
   Can_cs->cd();
   Can_cs->SetSelected(Can_cs);
